<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="product_template_form_view" model="ir.ui.view">
            <field name="name">product.template.form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='barcode']" position="after">
                    <field name="asin" groups="syncspider.group_asin"/>
                    <field name="distribution_channel_tag_ids" widget="many2many_tags" options="{'color_field': 'color'}" groups="syncspider.group_distribution_channels"/>
                </xpath>
            </field>
        </record>

        <record id="product_normal_form_view_project_s" model="ir.ui.view">
            <field name="name">product.product.form</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='barcode']" position="after">
                    <field name="asin" groups="syncspider.group_asin"/>
                    <field name="distribution_channel_tag_ids" widget="many2many_tags" options="{'color_field': 'color'}" groups="syncspider.group_distribution_channels"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>