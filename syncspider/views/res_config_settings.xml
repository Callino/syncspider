<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.sale</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="10"/>
            <field name="inherit_id" ref="sale.res_config_settings_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//div[@name='catalog_setting_container']" position="inside">
                    <div class="col-12 col-lg-6 o_setting_box" id="product_asin">
                        <div class="o_setting_left_pane">
                            <field name="group_asin"/>
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="group_asin"/>
                            <div class="text-muted">
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box" id="product_distribution_channels">
                        <div class="o_setting_left_pane">
                            <field name="group_distribution_channels"/>
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="group_distribution_channels"/>
                            <div class="text-muted">
                            </div>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>

    </data>
</odoo>